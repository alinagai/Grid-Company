@model GridCom.Models.RES

@{
    ViewData["Title"] = "Детали";
}

<h1>Информация</h1>

<div>
    <h4>РЭС</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            РЭС
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name_RES)
        </dd>
        <dt class = "col-sm-2">
            Полное название РЭС
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FullName_RES)
        </dd>
        <dt class = "col-sm-2">
           Адрес
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Adress_RES)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.RESID">Изменить</a> |
    <a asp-action="Index">Назад</a>
</div>
