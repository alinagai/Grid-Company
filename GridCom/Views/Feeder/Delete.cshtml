@model GridCom.Models.Feeder

@{ ViewData["Title"] = "Удаление"; }


<h3>Вы уверены, что хотите удалить запись?</h3>
<div>
    <h4>Список фидеров</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
           Подстанция
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Substations.Name_PS)
        </dd>
        <dt class="col-sm-2">
           Номер фидера
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Num_feeder)
        </dd>
        <dt class="col-sm-2">
            Протяженность, км
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Extent_feeder)
        </dd>

    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="FeederID" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Назад</a>
    </form>
</div>
