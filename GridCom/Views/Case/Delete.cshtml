@model GridCom.Models.CasesOfOutage

@{
    ViewData["Title"] = "Удаление";
}

<h3>Вы уверены, что хотите удалить запись?</h3>
<div>
    <h4>Причина отключения</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           Причина
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Case_outage)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="CasesOfOutageID" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Назад</a>
    </form>
</div>
